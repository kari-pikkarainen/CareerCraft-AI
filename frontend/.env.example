# CareerCraft AI Frontend Environment Configuration
# Copy this file to .env.local and update with your values

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_API_KEY=your-api-key-here
REACT_APP_API_SECRET=your-api-secret-here

# Environment
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_REAL_TIME_UPDATES=true

# UI Configuration
REACT_APP_DEFAULT_THEME=system
REACT_APP_ITEMS_PER_PAGE=20
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=pdf,docx,txt

# Development/Debug
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info